var start = clock();

fun fib(n) {
    if (n < 0) {
        return -1;
    }
    fun _fib(_n, _a, _b) {
        if (_n == 0) {
            return _a;
        }
        if (_n == 1) {
            return _b;
        }
        return _fib(_n - 1, _b, _a + _b);
    }
    return _fib(n, 0, 1);
}

print(fib(20));

var end = clock();
print(end - start);

